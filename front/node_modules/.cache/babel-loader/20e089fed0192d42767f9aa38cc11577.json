A,YAAA,CAAyB,IAAzB,EAAyB,IAAzB,EAAyB,gBAAA,CAAnB,OAAO,CAAC,IAAW,CAAzB,EAAkB;AAAA;AAAlB,K,EACA,YAAA,CAAgC,IAAhC,EAAgC,IAAhC,EAAgC,gBAAA,CAA1B,OAAO,CAAC,WAAkB,CAAhC,EAAyB;AAAA;AAAzB,K,EACA,YAAA,CAA0B,IAA1B,EAA0B,IAA1B,EAA0B,gBAAA,CAApB,OAAO,CAAC,KAAY,CAA1B,EAAmB;AAAA;AAAnB,K,EACA,YAAA,CAA0B,IAA1B,EAA0B,IAA1B,EAA0B,gBAAA,CAApB,OAAO,CAAC,KAAY,CAA1B,EAAmB;AAAA;AAAnB,K,EACA,YAAA,CAAsG,IAAtG,EAAsG,IAAtG,EAAsG,CAAlG,YAAA,CAA6F,GAA7F,EAA6F;AAAzF,MAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,eAAE,KAAA,CAAA,SAAA,GAAS,IAAX;AAAA,OAAA;AAAoF,KAA7F,E,CAA6B,W,CAA7B,CAAkG,CAAtG,C,EACA,YAAA,CAAqH,IAArH,EAAqH,IAArH,EAAqH,CAAjH,YAAA,CAA4G,GAA5G,EAA4G;AAApG,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,SAAA,CAAU,OAAO,CAAC,EAAlB,CAAF;AAAA;AAA+F,KAA5G,E,CAAsC,W,CAAtC,E;;AAAA,M,WAAA,CAAiH,CAArH,C,CAPF,C;GAQK,CARL,E;;AAAA,GASM,EAVR,CAWM,CAvBR,CAwBE,CAtDR,C","sourcesContent":["<template>\n  <div>\n        <h1>Catalog</h1>\n        <modal v-if=\"showModal\" @close=\"showModal = false\">\n          <form class=\"field-set\" method=\"POST\" v-on:submit.prevent=\"submitForm\">\n            <button class=\"close\" @click=\"showModal = false\">\n              <span aria-hidden=\"true\">&times;</span>\n            </button>\n            <label for=\"name\" class=\"input-title\">Name :</label>\n            <br>\n            <input class=\"form-control mr-sm-2 dark-input-color\"  type=\"text\"  id=\"name\" name=\"name\" v-model=\"form.name\">\n            <br>\n            <br>\n            <label for=\"description\" class=\"input-title\">Description :</label>\n            <br>\n            <input class=\"form-control mr-sm-2 dark-input-color\"  type=\"text\" id=\"description\" name=\"description\" v-model=\"form.decription\">\n            <br>\n            <br>\n            <label for=\"photo\" class=\"input-title\">Photo :</label>\n            <br>\n            <input class=\"form-control mr-sm-2 dark-input-color\"  type=\"text\" id=\"photo\" name=\"photo\" v-model=\"form.photo\">\n            <br>\n            <br>\n            <label for=\"price\" class=\"input-title\">Price :</label>\n            <br>\n            <input class=\"form-control mr-sm-2 dark-input-color\"  type=\"text\" id=\"price\" name=\"price\" v-model=\"form.price\">\n            <br>\n            <br>\n            <button class=\"btn btn-primary submit-theme-button\">Submit</button>\n          </form>\n        </modal>\n        <table>\n          <thead>\n            <tr>\n              <th>Id</th>\n              <th>Name</th>\n              <th>Description</th>\n              <th>Photo</th>\n              <th>Price</th>\n              <th>Edit</th>\n              <th>Delete</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"product in catalog\" :key=\"product.id\">\n              <td>{{product.id}}</td>\n              <td>{{product.name}}</td>\n              <td>{{product.description}}</td>\n              <td>{{product.image}}</td>\n              <td>{{product.price}}</td>\n              <td><a @click=\"showModal = true\"><img alt=\"edit\" src=\"@/assets/edit.png\" class=\"edit-button\"></a></td>\n              <td><a v-on:click=\"deleteRaw(product.id)\"><img alt=\"delete\" src=\"@/assets/delete.png\" class=\"delete-button\"></a></td>\n            </tr>\n          </tbody>\n        </table>\n    </div>\n</template>\n\n<script>\n\n  import axios from 'axios';\n  export default {\n    name: 'catalog',\n    data (){\n      return {\n        showModal: false,\n        form : {\n            name: '',\n            description: '',\n            photo: '',\n            price: ''\n        },\n        catalog: {\n\n        }\n      }\n    },\n    mounted() {\n      axios\n        .get('http://10.0.2.15/api/products')\n        .then((response) => {\n          this.catalog = response.data,\n          console.warn(\"get products :\", this.catalog)\n        })\n        .catch(error => console.log(error))\n    },\n    methods: {\n      edit: function () {\n\n      },\n      deleteRaw: function (productId) {\n        axios\n          .delete('http://10.0.2.15/api/product/' + { productId })\n          .catch(error => console.log(error))\n        alert('raw deleted')\n      },\n      submitForm: function () {\n        alert(this.form)\n      }\n    }\n  }\n\n</script>\n\n<style scoped>\n\n  table {\n    height: 80%;\n    width: 80%;\n    display: relative;\n    text-align: center;\n    border-radius: 3px;\n    border: 1px solid lightgray;\n    background-color: #343a40;\n    color: white;\n  }\n\n  thead {\n      border: 1px solid lightgray;\n  }\n\n  tbody {\n    color : grey;\n  }\n\n  tr {\n    margin-bottom: 50px;\n  }\n\n  .edit-button {\n    height: 20px;\n  }\n\n  .delete-button {\n    height: 25px;\n  }\n\n  form {\n    width: 450px;\n    min-height: 500px;\n    height: auto;\n    border-radius: 5px;\n    margin: 2% auto;\n    box-shadow: 0 9px 50px hsla(20, 67%, 75%, 0.31);\n    padding: 2%;\n    background-image: linear-gradient(-225deg, #212b37 50%, #3b3b3b 50%);\n    box-shadow:  0 0 30px black;\n  }\n\n  .input-title{\n    color: white;\n  }\n\n  .dark-input-color {\n      background-color: black;\n      color: lightgray;\n      border-color: #1fcfc6;\n  }\n\n  .submit-theme-button {\n    background-color: rgb(71, 71, 71);\n    color: lightgray;\n    border-color: #1fcfc6;\n  }\n\n</style>\n"],"sourceRoot":""}]}